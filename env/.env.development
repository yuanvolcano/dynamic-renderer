# 微信小程序配置
VITE_MP_WEIXIN_APP_ID=your_dev_app_id

# H5 配置
VITE_H5_ROUTER_BASE=/
VITE_H5_PUBLIC_PATH=/

# 构建配置
VITE_DROP_CONSOLE=false
VITE_DROP_DEBUGGER=false
VITE_DEV_PORT=3000
