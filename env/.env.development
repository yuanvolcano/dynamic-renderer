# 开发环境配置
NODE_ENV=development
VITE_APP_TITLE=跨平台动态UI构建平台 - 开发环境
VITE_APP_VERSION=1.0.0
VITE_APP_ENV=development

# API 配置
VITE_API_BASE_URL=http://localhost:8080
VITE_API_TIMEOUT=10000

# 调试相关
VITE_APP_DEBUG=true
